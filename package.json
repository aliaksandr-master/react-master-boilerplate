  "scripts": {
    "ver": "echo \"node: $(node -v), npm: v$(npm -v)\"",
    "open": "NODE_ENV=local-development node_modules/.bin/gulp dev/open-browser",
    "ci-lint": "npm run lint",
    "ci-unit": "npm run jest -- --config=./jest-quick.json --maxWorkers=8 --logHeapUsage --no-colors --silent --bail",
    "ci-build": "npm run test-build",
    "ci-deploy-dev": "NODE_ENV=development node_modules/.bin/gulp deploy --entries=index-main --target=dev",
    "ci-deploy-dev-min": "NODE_ENV=production node_modules/.bin/gulp deploy --entries=index-main --target=prod --minify",
    "test": "npm run lint && npm run jest -- --config=./jest-quick.json --maxWorkers=8 --logHeapUsage --no-colors --silent --bail --ci",
    "lint": "NODE_ENV=development node_modules/.bin/gulp test-code-style",
    "test-build": "NODE_ENV=production node_modules/.bin/gulp test-build --target=prod --no-progress --entries=index-main --no-source-map",
    "jest": "NODE_ENV=development node_modules/.bin/jest",
    "jest-watch": "npm run jest -- --colors --cache --coverage --updateSnapshot --watchAll",
    "cleanup": "npm i && npm update && npm prune && npm dedupe && node_modules/.bin/gulp cleanup",
    "prepush": "npm run test",
    "precommit": "npm run lint -- --test-tracked=true",
    "postmerge": "npm run cleanup",
    "postcheckout": "npm run cleanup",
    "action": "node_modules/.bin/yo ./dev/actions",
    "gen": "npm run action",
    "start": "NODE_ENV=local-development node_modules/.bin/gulp dev --target=local --progress --source-map",
    "start-sandbox": "NODE_ENV=local-development node_modules/.bin/gulp dev --target=local --progress --source-map --entries=index-sandbox",
    "pour-prod": "NODE_ENV=development node_modules/.bin/gulp pour --entries=index-main --target=prod",
    "publish-dev": "NODE_ENV=development node_modules/.bin/gulp release --entries=index-main --target=dev",
    "publish-dev-min": "NODE_ENV=production node_modules/.bin/gulp deploy --entries=index-main --target=dev --minify",
    "publish-prod": "NODE_ENV=production node_modules/.bin/gulp release --entries=index-main --target=prod --minify",
    "release-patch": "git push && git checkout master && git merge origin/development --no-ff --no-edit && npm test && npm version patch && npm run publish-prod -- --force-release && git push origin master --tags --no-verify && git checkout development && git pull && git merge origin/master --no-ff --no-edit --no-verify && git push origin development --no-verify && echo 'done!'",
    "release-minor": "git push && git checkout master && git merge origin/development --no-ff --no-edit && npm test && npm version minor && npm run publish-prod -- --force-release && git push origin master --tags --no-verify && git checkout development && git pull && git merge origin/master --no-ff --no-edit --no-verify && git push origin development --no-verify && echo 'done!'",
    "release-major": "git push && git checkout master && git merge origin/development --no-ff --no-edit && npm test && npm version major && npm run publish-prod -- --force-release && git push origin master --tags --no-verify && git checkout development && git pull && git merge origin/master --no-ff --no-edit --no-verify && git push origin development --no-verify && echo 'done!'"
  },
  "jest": {
    "automock": false,
    "clearMocks": true,
    "resetMocks": true,
    "roots": [
      "<rootDir>/app"
    ],
    "testURL": "http://jest.test/",
    "resetModules": true,
    "setupTestFrameworkScriptFile": "<rootDir>/dev/jest-setup/index.js",
    "setupFiles": [
      "<rootDir>/node_modules/jest-localstorage-mock",
      "<rootDir>/app/vendor/index.js",
      "<rootDir>/dev/jest-before-each/date.js",
      "<rootDir>/dev/jest-before-each/requestAnimationFrame.js",
      "<rootDir>/dev/jest-before-each/fetch.js",
      "<rootDir>/dev/jest-before-each/console.js",
      "<rootDir>/dev/jest-before-each/usymbol.js",
      "<rootDir>/dev/jest-before-each/unhandled-promise-rejection.js"
    ],
    "testEnvironment": "jsdom",
    "cacheDirectory": ".tmp/jest-cache/package",
    "collectCoverage": false,
    "coverageDirectory": ".var/test-coverage-report",
    "collectCoverageFrom": [
      "app/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__[^/]+?__/",
      "/dev/",
      "/app/vendor/",
      "/app/images/",
      "/app/prop-types/",
      "/app/components/test/.+$",
      "/app/components/.+?sandbox.js$",
      "/app/.+?mock.js$",
      "/app/index-sandbox\\.js$",
      "/app/index-main\\.js$",
      "/app/dev-menu\\.js$",
      "Sandbox"
    ],
    "globals": {
      "__ASSERTS_ENABLED__": true,
      "CONFIG": {
        "API_URL": "http://jest.test/",
        "BASE_URL": "/",
        "STATIC_URL": "/",
        "HISTORY": true
      },
      "process.env.NODE_ENV": "development"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!react-composite-router/|str-map/|constantin/|dom-tricks/|react-updating-hoc/|multi-routing-api/|react-redux-self/|react-fractal-field/)"
    ],
    "moduleFileExtensions": [
      "",
      "js",
      "jsx"
    ],
    "testRegex": "/__tests__/[^_]+.+?\\.test\\.js$",
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "react-highcharts": "<rootDir>/dev/jest-mocks/react-highcharts.jsx",
      "react-modal": "<rootDir>/dev/jest-mocks/react-modal.jsx",
      "material-ui/LinearProgress": "<rootDir>/dev/jest-mocks/react-component.js",
      "clipboard/dist/clipboard": "<rootDir>/dev/jest-mocks/clipboard.js",
      "react-dropzone": "<rootDir>/dev/jest-mocks/react-dropzone.js"
    },
    "transform": {
      "\\.jsx?$": "babel-jest",
      "\\.svg$": "<rootDir>/dev/jest-mocks/transform-file-to-component.js",
      "\\.(jpg|jpeg|png|gif)$": "<rootDir>/dev/jest-mocks/fileTransformer.js"
    }
  },
  "false": {}
}
